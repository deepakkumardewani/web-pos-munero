    "postcss": "^8.4.33",
    "prettier": "3.0.3",
    "sass": "^1.69.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.3.0",
    "unplugin-fonts": "^1.1.0",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.0.0",

    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/vue-fontawesome": "^3.0.5",
    "@mdi/font": "7.0.96",
    "@vueuse/core": "^10.7.1",
    "axios": "^1.6.5",
    "js-sha512": "^0.8.0",
    "moment": "^2.30.1",
    "pinia": "^2.1.7",
    "qrcode": "^1.5.3",
    "roboto-fontface": "*",
